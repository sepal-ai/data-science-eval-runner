{
  "name": "agent-eval-claude",
  "version": "1.0.0",
  "description": "Claude agent evaluation with computer-use capabilities",
  "type": "module",
  "main": "dist/src/claude-agent-cli.js",
  "scripts": {
    "build": "tsc",
    "run-agent-eval": "tsx --env-file=.env.local src/claude-agent-cli.ts",
    "dev": "tsx watch --env-file=.env.local src/claude-agent-cli.ts",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "bin": {
    "run-agent-eval": "./agent/cli/claude-cli.ts"
  },
  "author": "Sepal AI",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "axios": "^1.7.2",
    "dotenv": "^16.5.0"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4"
  }
}